{
  "connection": "default",
  "collectionName": "employmenthistory",
  "info": {
    "name": "employmenthistory",
    "description": "Employment History Table"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "institution": {
      "type": "string",
      "required": true
    },
    "start_year": {
      "type": "integer",
      "required": true
    },
    "end_year": {
      "type": "integer"
    },
    "doctors": {
      "model": "doctors",
      "via": "employmenthistories"
    }
  }
}